    "@nrwl/workspace": "8.9.0",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "24.0.9",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "cpx": "^1.5.0",
    "cypress": "3.4.0",
    "dotenv": "^6.2.0",
    "dotenv-extended": "^2.4.0",
    "jest": "24.1.0",
    "jest-preset-angular": "7.0.0",
    "prettier": "1.16.4",
    "sass-lint": "^1.13.1",
    "swagger-ui-express": "^4.0.7",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.18.0",
